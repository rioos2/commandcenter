<div class="login">
    <div class="container">
        <div class="row">
            <div class="logo-div col-md-12">
                <a href="#"><img class="logo-img tuna-signup-logo img-responsive center-block" src="images/logo1.png"></a>
            </div>
        </div>
        <div class="row content">
            <div class="col-lg-8 col-md-10 col-sm-12 col-md-offset-1 col-lg-offset-2 middle-div">
                <div class="center-block main-div col-md-12">
                    <div class="row">
                        <h4 class="title">METHODS OF SECURITY</h4>
                    </div>
                    <div class="row main-container">
                        <div class="stepwizard">
                            <div class="stepwizard-row setup-panel">
                                <div class="stepwizard-step col-xs-2 col-xs-offset-1">
                                    <a href="#step-1" type="button" class="step1 pending btn btn-success btn-circle"><div class="svg-pincode">{{theme-svg-pincode}}</div></a>
                                    <p><small>Pin Code</small></p>
                                </div>
                                <div class="stepwizard-step col-xs-2">
                                    <a href="#step-2" type="button" class="step2 pending select-btn  btn btn-default btn-circle" disabled="disabled"><div class="svg-face">{{theme-svg-face}}</div></a>
                                    <p><small>Facial scan</small></p>
                                </div>
                                <div class="stepwizard-step col-xs-2">
                                    <a href="#step-3" type="button" class="step3 pending btn btn-default btn-circle" disabled="disabled"><div class="svg-retina">{{theme-svg-retina}}</div></a>
                                    <p><small>Retina scan</small></p>
                                </div>
                                <div class="stepwizard-step col-xs-2">
                                    <a href="#step-4" type="button" class="step4 pending btn btn-default btn-circle" disabled="disabled"><div class="svg-finger">{{theme-svg-finger}}</div></a>
                                    <p><small>Fingerprint</small></p>
                                </div>
                                <div class="stepwizard-step col-xs-2">
                                    <a href="#step-5" type="button" class="step5 pending btn btn-default btn-circle" disabled="disabled"><div class="svg-camera">{{theme-svg-camera}}</div></a>
                                    <p><small>live video connection</small></p>
                                </div>
                            </div>
                        </div>

                        <form role="form">
                            <div class="panel-primary setup-content" id="step-1">
                                <div class="panel-body">
                                    <div class="form-group">
                                        <label class="control-label">First Name</label>
                                        <input maxlength="100" type="text" required="required" class="form-control" placeholder="Enter First Name" />
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label">Last Name</label>
                                        <input maxlength="100" type="text" required="required" class="form-control" placeholder="Enter Last Name" />
                                    </div>
                                    <button class="btn btn-primary nextBtn pull-right" type="button">Next</button>
                                </div>
                            </div>

                            <div class=" panel-primary setup-content" id="step-2">
                                <div class="panel-body">
                                    <h4 class="center-align">Validating face please wait...</h4>
                                    <div class="form-group">
                                        <input maxlength="100" type="hidden" class="form-control" placeholder="Enter First Name" value="123" />
                                    </div>
                                    <div class="row center-align">
                                        <button type="button" class=" btn nextBtn  bouton-image monBouton">VERIFICATION</button>
                                    </div>
                                    <div class="row center-align sign-div">
                                        <button class=" btn  bouton-image1 monBouton1">Sign up</button>
                                        <a href="#" class="signin">Sign in</a>
                                    </div>
                                    <div class="row center-align">
                                        <h5>Lorem ipsum dolor sit amet, consectetur adipiscing edit, sed do elusmod tempor</h5>
                                    </div>
                                </div>
                            </div>

                            <div class="panel panel-primary setup-content" id="step-3">
                                <div class="panel-heading">
                                    <h3 class="panel-title">Schedule</h3>
                                </div>
                                <div class="panel-body">
                                    <div class="form-group">
                                        <label class="control-label">Company Name</label>
                                        <input maxlength="200" type="text" class="form-control" placeholder="Enter Company Name" />
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label">Company Address</label>
                                        <input maxlength="200" type="text" class="form-control" placeholder="Enter Company Address" />
                                    </div>
                                    <button class="btn btn-primary nextBtn pull-right" type="button">Next</button>
                                </div>
                            </div>

                            <div class="panel panel-primary setup-content" id="step-4">
                                <div class="panel-heading">
                                    <h3 class="panel-title">Cargo</h3>
                                </div>
                                <div class="panel-body">
                                    <div class="form-group">
                                        <label class="control-label">Company Name</label>
                                        <input maxlength="200" type="text" required="required" class="form-control" placeholder="Enter Company Name" />
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label">Company Address</label>
                                        <input maxlength="200" type="text" required="required" class="form-control" placeholder="Enter Company Address" />
                                    </div>
                                    <button class="btn btn-primary nextBtn pull-right" type="button">Next</button>
                                </div>
                            </div>

                            <div class="panel panel-primary setup-content" id="step-5">
                                <div class="panel-heading">
                                    <h3 class="panel-title">Cargo</h3>
                                </div>
                                <div class="panel-body">
                                    <div class="form-group">
                                        <label class="control-label">Company Name</label>
                                        <input maxlength="200" type="text" required="required" class="form-control" placeholder="Enter Company Name" />
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label">Company Address</label>
                                        <input maxlength="200" type="text" required="required" class="form-control" placeholder="Enter Company Address" />
                                    </div>
                                    <button class="btn btn-success pull-right" type="submit">Finish!</button>
                                </div>
                            </div>
                        </form>

                    </div>
                    <div class="row">
                    </div>
                </div>


            </div>

        </div>
    </div>
</div>