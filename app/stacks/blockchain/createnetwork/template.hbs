<div class="create-cloud">
    <div class="container">
        {{#link-to "infrastructure-tab.data-center"}}<img class="logo-img" src="assets/images/logo2.png">{{/link-to}}
        <div class="col-container">
            <div class="col-right">
                {{!-- select cloud type --}}
                {{createblockchain-network/select-compute-for-network class="cloud-type cblock" model=model}}
                {{!-- select domain name --}}
                {{createblockchain-network/select-blockchain-network-peers class="select-domain cblock" model=model}}
                {{!-- select locaation --}}
                {{createcloud/select-location class="select-location cblock" model=model}}
                {{!-- cloud performance --}}
                {{createcloud/cloud-performance class="cloud-performance cblock" model=model}}
                {{!-- cloud server os --}}
                {{createblockchain-network/select-network-install-type class="server-os cblock" model=model}}
                {{!-- networking --}}
                {{createcloud/cloud-network-chooser class="networking cblock" model=model}}
                {{!-- summary --}}
                {{createcloud/cloud-summary class="summary cblock" model=model networking=model.networking}}
                {{!-- footer --}}
                {{page-footer}}
            </div>
        </div>
    </div>
</div>
