<div class="user">
    <div class="userinfo">
        <div class="row">
            <div class="col-sm-6 col-md-8 person clearfix">
                {{fa-icon "cog" class="settings"}}
                <img src={{img}}>
                <div class="avata">
                    <p class="name" style="text-transform: capitalize;">{{model.profile.first_name}}</p>
                    <div class="state {{model.state}}">
                        {{fa-icon "circle"}}
                        <p>{{model.state}}</p>
                    </div>
                </div>
            </div>
            <div class="col-sm-6 col-md-4 certification">
                {{svg/svg-hex-certification class="certi-icon"}}
                <p class="certisficated_date">{{createdAt}}</p>
                <p class="certisficated">Certisficated</p>
            </div>
        </div>
    </div>
    {{theme-alert alert=model.alert}}
    {{theme-table model=tableData class="info-table"}}
</div>
{{theme-footer}}
