<div class="servertab row m-0">
    <div class="col-md-12 col-lg-2 tab-left">
        <ul class="nav nav-pills brand-pills nav-stacked" role="tablist">
            <li role="presentation" class="brand-nav active" data-tooltip="User"><a href="#tab{{model.id}}{{type}}1" aria-controls="tab1" role="tab" data-toggle="tab" class="tabuser">{{svg/svg-user}}</a></li>
            <li role="presentation" class="brand-nav" data-tooltip="Speed"><a href="#tab{{model.id}}{{type}}2" aria-controls="tab2" role="tab" data-toggle="tab" class="tabspeed">{{svg/svg-speed}}</a></li>
            <li role="presentation" class="brand-nav" data-tooltip="State"><a href="#tab{{model.id}}{{type}}3" aria-controls="tab4" role="tab" data-toggle="tab" class="tabstats">{{svg/svg-stats}}</a></li>
            <li role="presentation" class="brand-nav" data-tooltip="Security"><a href="#tab{{model.id}}{{type}}4" aria-controls="tab3" role="tab" data-toggle="tab" class="tabsecur">{{svg/svg-secur}}</a></li>
            <li role="presentation" class="brand-nav ul-desc">
                <div class="p-l-7">
                    <div class="choose-desc">{{t 'stackPage.admin.node.choose'}}</div>
                    <div class="category-desc">{{t 'stackPage.admin.node.category'}}</div>
                </div>
            </li>
        </ul>
    </div>
    <div class="col-md-12 col-lg-10 tab-right">
        <div class="tab-content">
            <div role="tabpanel" class="tab-pane active" id="tab{{model.id}}{{type}}1">
                {{infra-node-visualizecpu model=model nodeType=type}}
            </div>
            <div role="tabpanel" class="tab-pane" id="tab{{model.id}}{{type}}2">
                {{infra-node-network-chart model=model nodeType=type}}
            </div>
            <div role="tabpanel" class="tab-pane" id="tab{{model.id}}{{type}}3">
                {{infra-node-process-chart model=model nodeType=type}}
            </div>
            <div role="tabpanel" class="tab-pane" id="tab{{model.id}}{{type}}4">
                {{infra-node-disk-column-chart model=model nodeType=type}}
            </div>
        </div>
    </div>
</div>
