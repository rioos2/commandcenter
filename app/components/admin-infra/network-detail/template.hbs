<div class="network-box-outer">
  <div class="network-box {{active}}">
    <div class="network-subbox-1">
      <div class="pull-left st-name">
        {{virtualNetworkName}}
      </div>
      <div class="pull-right st-pin">
        <span data-detail="#sd_detail1" class="detail-magic"><img src="assets/images/admin/storage-pin.png" alt="network-pin"></span>
      </div>
    </div>
    <div class="network-subbox-2">
      <span class="sub-title">{{t 'dojos.dashboard.networks.details.range'}}</span>
      <span class="sub-name">{{ip}}</span>
      <br>
      <span class="sub-title">{{t 'dojos.dashboard.networks.details.type'}}</span>
      <span class="sub-name">{{type}}</span>
    </div>
  </div>
</div>
