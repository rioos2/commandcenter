
					<div class="panel panel-default kv-panel">
						  <div class="panel-heading">
							<div class="panel-header">
								Connected Storages ({{count}})
							</div>
							<div class="pull-right kv-right-head-button">
								<button type="button" data-toggle="modal" data-target="#addstorage_modal" class="btn">+ Add</button>


							</div>
						  </div>
						  <div class="panel-body">
								{{#if NoStorage}}
								<div class="empty-network">
									<div class="pin-img">
										<img src="./images/3pin.png" class="img-responsive" alt="pin">
									</div>


									<div class="empty-network-add-link">
										<span>No storage added yet.</span>
										<a href="#">+Add Storage</a>
									</div>
								</div>
								{{else}}
								{{#each storages as |storage|}}
									{{admin-infra/storage-single model=storage}}
								{{/each}}

								{{/if}}

						  </div>
					</div>
