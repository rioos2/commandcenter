<div class="modal fade" id="addstorage_modal" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal">&times;</button>
      <h4 class="modal-title">{{t 'stackPage.admin.storage.addStorageTwo'}}</h4>
    </div>
    <div class="modal-body">
      <form action="#">
          <div class="row">
          <div class="col-sm-6">
            <div class="form-group">
              <label for="modal_st_dropdown">{{t 'stackPage.admin.storage.select'}}</label>
               <select class="form-control" id="modal_st_dropdown" name="modal_st_dropdown">
                <option>1</option>
                <option>2</option>
                <option>3</option>
                <option>4</option>
                </select>
            </div>
            <div class="form-group">
              <label for="modal_st_ip_slocation">{{t 'stackPage.admin.storage.ipAddress'}}</label>
              <input type="text" class="form-control" id="modal_st_ip_slocation" name="modal_st_ip_slocation">
            </div>
            <div class="form-group">
              <label for="modal_st_auth_password">{{t 'stackPage.admin.storage.authPass'}}</label>
              <input type="text" class="form-control" id="modal_st_auth_password" name="modal_st_auth_password">
            </div>
          </div>
          <div class="col-sm-6">
            <div class="form-group">
              <label for="modal_st_name">{{t 'stackPage.admin.storage.name'}}</label>
              <input type="text" class="form-control" id="modal_st_name" name="modal_st_name">
            </div>
            <div class="form-group">
              <label for="modal_st_auth_name">{{t 'stackPage.admin.storage.authuser'}}</label>
              <input type="text" class="form-control" id="modal_st_auth_name" name="modal_st_auth_name">
            </div>
            <div class="form-group">
              <label for="modal_st_pool_name">{{t 'stackPage.admin.storage.pool.name'}}</label>
              <input type="text" class="form-control" id="modal_st_pool_name" name="modal_st_pool_name">
            </div>
          </div>
          </div>
          <button type="submit" class="btn btn-default">{{t 'stackPage.admin.header.add_btn'}}</button>
      </form>
    </div>
    </div>

  </div>
</div>
