<div class="setup-content" id="step-7">
  <p class="sub-title"> Server OS </p>
  <p class="sub-description"> Select preferred system</p>
  <div class="row m-b-30">
    <div class="col-xs-4 cgpu">
      <img src="images/second/{{model.assemblyfactory.component_collection.compute_type}}.png">
      <div class="description">
        <p class="topic1">{{model.assemblyfactory.component_collection.compute_type}}</p>
        <p class="topic2">CLOUD TYPE</p>
      </div>
      <img class="right-light" src="images/second/summary/right_light.png">
    </div>
    <div class="col-xs-4 domain-name">
      <div class="description">
        <p class="topic1">{{model.assemblyfactory.properties.domain}}</p>
        <p class="topic2">Domain Name</p>
      </div>
      <img class="display-line" src="images/second/summary/right_light.png">
    </div>
    <div class="col-xs-4">
      <div class="set-location">
        <img class="btn-location" src="images/second/locaion_ico.png">
        <!-- <span class="city">Ohio</span> -->
        <span class="country">{{model.assemblyfactory.properties.region}}</span>
        <p class="topic2">Region</p>
      </div>
    </div>
  </div>
  <div class="row m-b-30 storage">
    <div class="col-xs-6">
      <div class="s-ram">
        <p class="topic0">{{model.assemblyfactory.properties.cpu}}</p>
        <img src="images/second/summary/hexCore.png">
      </div>
      <div class="description">
        <p class="topic1">Cores</p>
        <p class="topic2">Number</p>
      </div>
      <img class="right-light p-r-30" src="images/second/summary/right_dark.png">
      <div class="description">
        <p class="topic3">{{model.assemblyfactory.properties.ram}}</p>
        <p class="topic2">Ram</p>
      </div>
      <img class="right-light p-l-30" src="images/second/summary/right_dark.png">
    </div>
    <div class="col-xs-6 s-storage">
      <img class="flash" src="images/second/summary/flash.png">
      <div class="description">
        <p class="topic1">FLASH</p>
        <p class="topic2">Storage type</p>
      </div>
      <img class="right-light p-r-30" src="images/second/summary/right_dark.png">
      <div class="description">
        <p class="topic4">{{model.assemblyfactory.properties.storage}}</p>
        <p class="topic2">Storage Capacity</p>
      </div>
    </div>
  </div>
  <div class="row ip-os">
    <div class="description">
      <p class="topic1">{{network}}</p>
      <p class="topic2">Networking</p>
    </div>
    <img class="right-light p-r-30" src="images/second/summary/right_light.png">
    <img src="images/second/summary/ico_{{distroNameFromPlan}}.png">
    <div class="btn-version"><img><span>{{model.assemblyfactory.properties.version}}</span></div>
    <img class="right-light p-r-30" src="images/second/summary/right_light.png">
    <div class="launch">
      <p class="title" {{action 'createAssemblyFactory'}} >Launch your cloud<img src="images/second/summary/ico_cloud.png"></p>
    </div>
  </div>
</div>
